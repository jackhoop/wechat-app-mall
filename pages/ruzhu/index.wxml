<view class="page">
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">门店名称</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输门店名称"/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">店铺地址</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="选择地址" />
                </view>
                <view class="weui-cell__ft">
                   <view class="weui-select weui-select_in-select-after">{{countries[countryIndex]}}</view>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">营业时间</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="营业时间"/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">门面头照</view>
                </view>
                 <view class="weui-cell__bd">
                </view>
                <view class="weui-cell__ft">
                      <view class="weui-uploader__input-box" style='margin-top: 7px;'>
                          <view class="weui-uploader__input" bindtap="chooseImage"></view>
                      </view>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">营业执照</view>
                </view>
               <view class="weui-cell__bd">
                </view>
                <view class="weui-cell__ft">
                
                      <view class="weui-uploader__input-box" style='margin-top: 7px;'>
                       <image class="weui-uploader__img" src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1545288618808&di=606bee3559fa9cd9befc68f721abe06c&imgtype=0&src=http%3A%2F%2Fa.hiphotos.baidu.com%2Fzhidao%2Fpic%2Fitem%2Ffaedab64034f78f0e71e49f879310a55b3191ca7.jpg" mode="aspectFill" />
                                <view class="weui-uploader__input" bindtap="chooseImage"></view>
                      </view>
                        
                </view>
            </view>
             <view class="weui-cell">
                <view class="weui-cell__bd">
                         <view class="weui-uploader__hd">
                            <view class="weui-uploader__title">商家介绍</view>
                        </view>
                        <view class="weui-uploader__hd">
                           <view class="weui-cell__bd">
                              <textarea class="weui-textarea" placeholder="请填写商家介绍" style="height: 4.3em" />
                              <!-- <view class="weui-textarea-counter">0/200</view> -->
                          </view>
                        </view>
                        <view class="weui-uploader__bd">
                            <view class="weui-uploader__files" id="uploaderFiles">
                                <block wx:for="{{files}}" wx:key="*this">
                                    <view class="weui-uploader__file"  style="position: relative;margin-top: 7px;" bindtap="previewImage" id="{{item}}">
                                        <image class="weui-uploader__img" src="{{item}}" mode="aspectFill" />
                                       
                                         <view  data-index="{{index}}" catchtap="deleteImg" style="position: absolute;top: -.7em;right: -.4em;opacity:0.8;">
                                           <i class="icon iconfont icon-shanchu3" style="font-weight:bold;color:red;font-size:25rpx;"></i>
                                         </view>
                                    </view>
                                            
                                </block>
                              
                            </view>
                            <view class="weui-uploader__input-box" style='margin-top: 7px;'>
                                <view class="weui-uploader__input" bindtap="chooseImage"></view>
                            </view>
                        </view>
                 </view>
            </view>
             <view class="weui-cell">
                <view class="weui-cell__bd">
                        <view class="weui-uploader__hd">
                            <view class="weui-uploader__title">头部轮播图片</view>
                             <view class="weui-uploader__info">{{files.length}}/5</view>
                        </view>
                        <view class="weui-uploader__bd">
                            <view class="weui-uploader__files" id="uploaderFiles">
                                <block wx:for="{{files}}" wx:key="*this">
                                    <view class="weui-uploader__file"  style="position: relative;margin-top: 7px;" bindtap="previewImage" id="{{item}}">
                                        <image class="weui-uploader__img" src="{{item}}" mode="aspectFill" />
                                       
                                         <view  data-index="{{index}}" catchtap="deleteImg" style="position: absolute;top: -.7em;right: -.4em;opacity:0.8;">
                                           <i class="icon iconfont icon-shanchu3" style="font-weight:bold;color:red;font-size:25rpx;"></i>
                                         </view>
                                    </view>
                                            
                                </block>
                              
                            </view>
                            <view class="weui-uploader__input-box" style='margin-top: 7px;'>
                                <view class="weui-uploader__input" bindtap="chooseImage"></view>
                            </view>
                        </view>
                 </view>
                 
             </view>
        </view>
        <!-- <view class="weui-cells__tips">底部说明文字底部说明文字</view> -->
        <view class="weui-btn-area">
            <button class="weui-btn" type="primary" bindtap="showTopTips">提交审核</button>
        </view>
        <view class="weui-cells__tips"></view>
</view>